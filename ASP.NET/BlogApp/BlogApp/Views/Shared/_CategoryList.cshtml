@model IEnumerable<BlogApp.Models.Category>

<div class="well">
    <h4>Blog Kategorileri</h4>
    <div class="row">
        @foreach (var cat in Model)
        {
            <div class="col-lg-6">
                <ul class="list-unstyled">
                    <li>
                        <a href="/Blog/List/@cat.Id">@cat.Name</a>
                    </li>
                </ul>
            </div>
        }
    </div>
</div>

@{
    ViewBag.Title = "Index2";
}

<h2>Index2</h2>
@{
    List<String> isimler = new List<string>();
    isimler.Add("Enes");
    isimler.Add("Yusuf");
    isimler.Add("Baysan");
}
@{
    <ul>
        @foreach (string isim in isimler)
        {
            <li>
                @isim
            </li>
        }

    </ul>
}
<hr />
<ul>
    @for (int i = 0; i < 10; i++)
    {
        <li>
            i'nin şuanki değeri = @i
        </li>
    }
</ul>

